{
  "name": "@wandeber/quara",
  "version": "0.10.13-alpha",
  "description": "Query as Sara.",
  "license": "CC BY-ND 4.0",
  "keywords": [
    "quara",
    "language",
    "interpreter"
  ],
  "author": {
    "name": "Bernardo AlemÃ¡n Siverio",
    "email": "berthewanderer@gmail.com",
    "url": "https://github.com/wandeber"
  },
  "homepage": "https://github.com/wandeber/quara",
  "repository": {
    "type": "git",
    "url": "https://github.com/wandeber/quara"
  },
  "bugs": {
    "url": "https://github.com/wandeber/quara/issues"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "type": "module",
  "main": "./dist/Quara.cjs",
  "module": "./dist/Quara.js",
  "types": "./dist/Quara.d.ts",
  "exports": {
    "import": "./dist/Quara.js",
    "require": "./dist/Quara.cjs"
  },
  "scripts": {
    "start": "npm run build && npm run shell",
    "dev": "tsx --watch ./src/shell.ts",
    "shell": "NODE_NO_WARNINGS=1 node ./dist/Quara.shell.js",
    "build": "npm run clean; npm run build:tsup",
    "build-prod": "npm run clean; npm run build:tsup && npm run check",
    "preversion": "npm run check",
    "check": "npm test && npm run lint",
    "clean": "rm -rf ./dist",
    "build:tsup": "tsup --config tsup.config.cjs",
    "test": "./node_modules/.bin/jest --detectOpenHandles --forceExit --coverage",
    "test-verbose": "npm run test -- --verbose",
    "lint": "npx eslint",
    "format": "npx prettier --write .",
    "check-format": "npx prettier --check .",
    "gen-dist-npm": "./scripts/generate-dist-npm.sh",
    "test-module-import": "node ./scripts/test-module-import.cjs",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=18.17.1",
    "npm": ">=9.6.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/jest": "^29.5.12",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.2",
    "tsup": "^8.5.1",
    "tsx": "^4.19.4",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1"
  }
}
