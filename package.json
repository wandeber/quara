{
  "name": "quara",
  "version": "0.4.0",
  "description": "Query as Sara.",
  "author": "Bernardo A. Siverio (wandeber)",
  "main": "index.js",
  "scripts": {
    "start": "npm run rebuild && npm run quara",
    "quara": "npm run shell",
    "shell": "node ./dist/shell.js",
    "rebuild": "npm run clean && npm run build",
    "clean": "rm -r ./dist",
    "build": "npm run build-tsc && npm run lint",
    "build-tsc": "./node_modules/.bin/tsc",
    "lint": "./node_modules/.bin/eslint ./**/*.ts",
    "test": "jest --forceExit --coverage --verbose"
  },
  "keywords": [
    "query",
    "interpreter",
    "language",
    "sql",
    "database",
    "filter"
  ],
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "jest": "^26.4.0",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  }
}